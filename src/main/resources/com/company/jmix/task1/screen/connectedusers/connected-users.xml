<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://jmix.io/schema/ui/window"
        caption="msg://connectedUsers.caption">
    <layout spacing="true" margin="true">
        <vbox spacing="true">

            <jsComponent id="websocket"
                         initFunctionName="io_jmix_sampler_screen_ui_components_javascript_component_TimePicker"
            >
                <dependencies>
                    <dependency path="webjar://jquery:jquery.min.js"/>
                    <dependency path="webjar://sockjs-client:sockjs.min.js"/>
                    <dependency path="webjar://stomp-websocket:stomp.min.js"/>
                    <dependency path="vaadin://textarea/websocket_logic.js"/>
                </dependencies>
            </jsComponent>
            <textArea id="message" width="100%"
                      height="100%"
                      align="TOP_CENTER"
                      editable="false"
            />
        </vbox>
    </layout>
</window>